{
  "name": "@gorgonjs/file-provider",
  "version": "1.4.0",
  "description": "A simple file caching provider for Gorgon JS",
  "homepage": "https://gorgonjs.dev",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "vite build",
    "prepublish": "vite build",
    "tscheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mikevalstar/gorgon.git"
  },
  "keywords": [
    "cache",
    "promise",
    "async",
    "typescript",
    "filesystem"
  ],
  "author": "Mike Valstar <mike@valstar.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikevalstar/gorgon/issues"
  },
  "devDependencies": {
    "@gorgonjs/gorgon": "^1.3.1",
    "@types/node": "^18.11.9",
    "@vitest/coverage-c8": "^0.25.0",
    "eslint": "^8.11.0",
    "jest": "^27.5.1",
    "typescript": "^5.1.6",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.0",
    "vitest": "^0.25.0"
  },
  "jest": {
    "collectCoverage": true,
    "timers": "fake"
  },
  "peerDependencies": {
    "@gorgonjs/gorgon": "^1.4.0"
  },
  "dependencies": {
    "fast-glob": "^3.2.12"
  }
}
